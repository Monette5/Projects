            <li data-code="f199" data-class="fa fa-envelope-square" class="fa fa-envelope-square"></li>
            <li data-code="f041" data-class="fa fa-map-marker" class="fa fa-map-marker"></li>
           
            <!---Payment Icons-->
            <li data-code="f09d" data-class="fa fa-credit-card" class="fa fa-credit-card"></li>
          </ul>
        </div>

        <div id="triggericoncolorlabel" wix-ctrl="SectionDividerLabeled" wix-options="{label: 'Trigger Icon color', infoText:'Choose a color to trigger icon'}"></div>
        <div id="triggericoncolor" wix-param="triggericoncolor" wix-ctrl="ColorPickerInput" wix-options="{startWithColor: 'color-5', title: 'Trigger Icon color'}"></div>
        <hr class="divider-long"/>
        <div id="iconsize" class="iconsize" wix-param="iconsize" wix-ctrl="Slider" wix-options="{title: 'Trigger Icon Size', defaultValue:40, units: 'px', max : 65}"></div>
      </div>

      <div id="triggertextcontainer">
        <div id="cooltip_text_label" wix-ctrl="SectionDividerLabeled"  wix-options="{label: 'Cooltip/Trigger Text', infoText:'Cooltip text is revealed'}"></div>
        <div id="trigger-heading"  wix-ctrl="Input"  wix-options="{ title: 'Trigger Text', placeholder: 'Trigger Text', defaultText: '', maxLength: 40, invalidMessage: 'Max 40 characters' ,infoText:'you can put maximum 40 characters in trigger heading input'}" ></div>
        <hr class="divider-long"/>
        <div id="tooltip-heading"  wix-ctrl="Input"  wix-options="{ title: 'Cooltip Text',isMultiLine:true, placeholder: 'Cooltip Text', defaultText: '', maxLength: 100, invalidMessage: 'Max 100 characters',infoText:'you can put maximum 100 characters in tooltip text input' }" ></div>
      </div>

    </div>

  </div>
  <script>
    $(document).ready(function () {
      $('.drill_accordion > ul .drillaccordionlink').on('click', function () {
        
        $('.start_end_view').addClass('display-none');
        $("#particles_color_link").addClass('display-none');
        $("#settings_triggerlink_container").hide();
        $("#trans_color_link").hide();
       
        if (settings.transitiontype == "cooltips") {
          $(".images_filter").hide();
        }

        var drill_id = $(this).data('main');
        if (!drill_id) {
          return false;
        }
        var drill_item = $(this).data('item');
        $('#' + drill_id + ' > ul li.drillaccordionlink').hide();
        $('#' + drill_id + ' > ul li.drillaccordionback').show();
        $('#' + drill_id + ' > .drillaccordioncontent').hide();
        $('#' + drill_id + ' > .drillaccordioncontent.item_' + drill_item).show();

        /****Aritisian pack******/
        if (transition_type == 'ap1') {
            artisianPack();
        }

        /****Atomic pack ******/
        if (transition_type == 'codrops') {
          atomicPack();
        }

        /****Transition pack ******/
        if (transition_type == 'codyhouse') {
          transitionPack();
        }
        /****Transition pack ******/

        /****Trad pack ******/
        if (transition_type == 'trad') {
          tradPack();
        }
        /****Trad pack ******/

        if (drill_item != 2 && drill_item != 5 && drill_item != 8) {
           $('#' + drill_id + ' > .drillaccordionshowactive').show();
           if (transition_type!= 'ap1') {
            $('#image1').show();
           }
        }

        /****Trad pack ******/
        if (transition_type == 'cooltips') {
          // $(".start_end_toggle_level").show();
          if (drill_item == 1 || drill_item == 10) {
            $("#start-end-toggle").hide();
            $(".start_end_toggle_level").hide();
          }
        }

      });
      globaldesignSettings(settings.transitiontype);
    });

    function tradPack() {
      $("#image1").show();
    }

    function reload_drill(drill_id) {
      $('.start_end_view').removeClass('display-none');
     // $('#particles_color_link').removeClass('display-none');
      $('#' + drill_id + ' > ul li.drillaccordionlink').show();
      $('#' + drill_id + ' > ul li.drillaccordionback').hide();
      $('#' + drill_id + ' > .drillaccordioncontent').hide();
      $('#' + drill_id + ' > .drillaccordionshowactive').hide();
    }

    function artisianPack() {
     if(settings.startEndToggle == 2){
       $('.img-filter-label').hide();
     }
     
     $('.hide-filter-effect label input[value="2"]').parent().click('on',
        function () {
          $('.start_end_view').addClass('display-none');
          $('#image-zoom-on').hide();
          $('.img-filter-label').hide();
        });
      $('.hide-filter-effect label input[value="1"]').parent().click('on',
        function () {
          $('.start_end_view').addClass('display-none');
          $('.img-filter-label').show();
        });
    }

    function atomicPack() {
      $("#image-zoom-on").removeClass('display-none');
      $(".allowbuttons").show();
      showAtomic();

      hidesectionAtomic();
      $('.hide-filter-effect label input[value="1"]').parent().click('on',
        function () {
          showAtomic();
          hidesectionAtomic();
        });
      $('.hide-filter-effect label input[value="2"]').parent().click('on',
        function () {
          $('#image1').show();
          $('#overlay-on').show();
          $('#overlayOffAngleContainer').show();
          $('.content--intro').show();
          $(".img-filter-label").show();
        });
    }

    function showAtomic(){
       $('.img-back-color-off').show();
       $("#text-animation-off").show();
       $('#textbtn-radius-off').show();
    }

    function hidesectionAtomic() {
      $('#overlay-on').hide();
      $('#overlay-off').hide();
      $('#overlayOffAngleContainer').hide();
      $('#image0').hide();
      $("#image-zoom-off").hide();
      $(".img-filter-label").hide();
    }

    function transitionPack() {
      setTimeout(function () {
        $("#image0").show();
        $("#image-zoom-off").show();
        $('#overlay-off').show();
        $('.img-filter-label').show();
        $('#overlayOffAngleContainer').show();
      }, 1000);
    }

    function globaldesignSettings(transition_type) {
      
      /****Transition pack ******/
      if (transition_type == 'codyhouse') {
        $("#trans_color_link").show();
      }
      $('.hide-filter-effect label input[value="1"]').parent().click('on',
        function () {
          $('.start_end_view').addClass('display-none');
          $("#settings_triggerlink_container").hide();
        });

      $('.hide-filter-effect label input[value="2"]').parent().click('on',
        function () {
          $("#settings_triggerlink_container").show();
        });

      $('.drill_accordion > ul  > li.drillaccordionback > .backlink').on('click', function () {
        $('.start_end_view').show();
        $('.start_end_view').removeClass('display-none');
        if (!drill_id) {
          return false;
        }
        $('#' + drill_id + ' > ul li.drillaccordionlink').show();
        $('#' + drill_id + ' > ul li.drillaccordionback').hide();
        $('#' + drill_id + ' > .drillaccordioncontent').hide();
        $('#' + drill_id + ' > .drillaccordionshowactive').hide();
      });
    }
  </script>
</div>
