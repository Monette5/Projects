		function triggerlink(){ console.log(settings.linkData);

      if(settings.linkData) {
        Wix.navigateTo(settings.linkData);
      }
		}

		var stuck=false;
		var on=false;
		var cur_hover = false;

		function enter() {
			if(getParameterByName('viewMode')!='editor'){
					cur_hover = true;
					var transition='gooey-transition', rollover = false;
					if (settings.transition && settings.transition!='') {
							transition=settings.transition;
					} else {
							transition='gooey-transition';
					}
					rollover = rollovers[transition];
					on=true;
					if (!stuck) {
						rollover.animation_script_on(settings);
					}
			}
		}

		function leave() {
			if(getParameterByName('viewMode')!='editor'){
				cur_hover = false;
				var transition= 'gooey-transition', rollover = false;

				if (settings.transition && settings.transition!='') {
					transition=settings.transition;
				} else {
					transition='gooey-transition';
				}

				rollover = rollovers[transition];

				if (!stuck) {
					on = false;

					if (settings.sticky === true || settings.sticky === "true") {
						stuck = true;
					} else {
						rollover.animation_script_off(settings);
					}
				}

			}
		}

		// If there is no trigger on button allow transition hover
		if(settings.textbtna == false || settings.textbtna == "false"  || !settings.textbtna && getParameterByName('viewMode')!='editor') {

				$('body').mouseenter(enter);
				if(settings.endStateLink=="false"){
							$('#container').click('on',function(){
								return false;
							});
							$('#codrops').click('on',function(){
								return false;
							});
				}

		}

		if(settings.textbtna == false || settings.textbtna == "false" || !settings.textbtna && getParameterByName('viewMode')!='editor') {
			$('body').mouseleave(leave);
		}

		var toggle = function() {
		  if (!on) {
			  enter();
		  } else {
        if(settings.linkData == undefined || settings.linkData == '') {
          leave();
        } else {
						$('html').on('click','.target-link',function(){
							triggerlink();
						});
        }
      }
		}

    // Use click events for desktop and touch events for mobile
    if(getParameterByName('deviceType') == 'desktop') {
		  $('body').bind('click', toggle);
    } else {
      $('body').bind('touchstart', toggle);
    }

		function getInternetExplorerVersion(){
			var rv = -1;
			if (navigator.appName == 'Microsoft Internet Explorer'){
				var ua = navigator.userAgent;
				var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
				if (re.exec(ua) != null)
				rv = parseFloat( RegExp.$1 );
			}
			else if (navigator.appName == 'Netscape'){
				var ua = navigator.userAgent;
				var re  = new RegExp("Trident/.*rv:([0-9]{1,}[\.0-9]{0,})");
				if (re.exec(ua) != null)
				rv = parseFloat( RegExp.$1 );
			}
		  return rv;
		}

		function isFirefoxOrAndroidOrIPad1() {
		  return navigator.userAgent.indexOf('Firefox/')!=-1 || navigator.userAgent.indexOf('Android')!=-1 || isIPad1();
		}

		function isIPad1() {
		  return navigator.userAgent.indexOf('Apple-iPad/')!=-1 || navigator.userAgent.indexOf('Apple-iPad1C1/')!=-1;
		}

