  <script>
    $(document).ready(function () {
      $('.drill_accordion > ul .drillaccordionlink').on('click', function () {

        $('.start_end_view').addClass('display-none');
        //$("#settings_triggerlink_container").hide();
        $("#trans_color_link").hide();
        $('#end-image-show').hide();//new-ui
        $('#show-hide-font').hide();//new-ui
        $('#show-hide-text').hide();//new-ui
        $('#layout-options').hide();//new-ui
        $('#colour-filter-start').hide();//new-ui
        $('#colour-filters-end').hide();//new-ui
        $('videocontainer').hide();//new-ui
        //$('end-mobile').hide();//new-ui



        if (settings.transitiontype == "cooltips") {
          $(".images_filter").hide();
        }

        // $('.images_filter').hide();
        // $('#cooltips_color_link').hide();

        var drill_id = $(this).data('main');
        if (!drill_id) {
          return false;
        }
        var drill_item = $(this).data('item');
        $('#' + drill_id + ' > ul li.drillaccordionlink').hide();
        $('#' + drill_id + ' > ul li.drillaccordionback').show();
        $('#' + drill_id + ' > .drillaccordioncontent').hide();
        $('#' + drill_id + ' > .drillaccordioncontent.item_' + drill_item).show();

        /****Aritisian pack******/
        if (transition_type == 'ap1') {
          setTimeout(function () {
            artisianPack();
          }, 500);
        }

        /****Atomic pack ******/
        if (transition_type == 'codrops') {
          atomicPack();
        }

        /****Transition pack ******/
        if (transition_type == 'codyhouse') {
          transitionPack();
        }
        /****Transition pack ******/

        /****Trad pack ******/
        if (transition_type == 'trad') {
          tradPack();
        }
        /****Trad pack ******/

        if (drill_item != 2 && drill_item != 5 && drill_item != 8) {
          $('#' + drill_id + ' > .drillaccordionshowactive').show();
          $('#image1').show();
        }

        /****Trad pack ******/
        if (transition_type == 'cooltips') {
          // $(".start_end_toggle_level").show();
          if (drill_item == 1 || drill_item == 10) {
            $("#start-end-toggle").hide();
            $(".start_end_toggle_level").hide();
          }
        }

      });
      globaldesignSettings(settings.transitiontype);
    });

    function tradPack() {
      $("#image1").show();
    }

    function reload_drill(drill_id) {
      $('.start_end_view').removeClass('display-none');
      $('#' + drill_id + ' > ul li.drillaccordionlink').show();
      $('#' + drill_id + ' > ul li.drillaccordionback').hide();
      $('#' + drill_id + ' > .drillaccordioncontent').hide();
      $('#' + drill_id + ' > .drillaccordionshowactive').hide();
    }

    function artisianPack() {
      $('.hide-filter-effect label input[value="2"]').parent().click('on',
        function () {
          $('.start_end_view').addClass('display-none');
          $('#image-zoom-on').hide();
          $('.img-filter-label').hide();
        });
      $('.hide-filter-effect label input[value="1"]').parent().click('on',
        function () {
          $('.start_end_view').addClass('display-none');
          $('.img-filter-label').show();
        });
    }

    function atomicPack() {
      $("#image-zoom-on").removeClass('display-none');
      $(".allowbuttons").show();
      showAtomic();

      hidesectionAtomic();
      $('.hide-filter-effect label input[value="1"]').parent().click('on',
        function () {
          showAtomic();
          hidesectionAtomic();
        });
      $('.hide-filter-effect label input[value="2"]').parent().click('on',
        function () {
          $('#image1').show();
          $('#overlay-on').show();
          $('#overlayOffAngleContainer').show();
          $('.content--intro').show();
          $(".img-filter-label").show();
        });
    }

    function showAtomic(){
       $('.img-back-color-off').show();
       $("#text-animation-off").show();
       $('#textbtn-radius-off').show();
    }

    function hidesectionAtomic() {
      $('#overlay-on').hide();
      $('#overlay-off').hide();
      $('#overlayOffAngleContainer').hide();
      $('#image0').hide();
      $("#image-zoom-off").hide();
      $(".img-filter-label").hide();
    }

    function transitionPack() {
      setTimeout(function () {
        $("#image0").show();
        $("#image-zoom-off").show();
        $('#overlay-off').show();
        $('.img-filter-label').show();
        $('#overlayOffAngleContainer').show();
      }, 1000);
    }

    function globaldesignSettings(transition_type) {

      /****Transition pack ******/
      if (transition_type == 'codyhouse') {
        $("#trans_color_link").show();
      }
      $('.hide-filter-effect label input[value="1"]').parent().click('on',
        function () {
          $('.start_end_view').addClass('display-none');
          //$("#settings_triggerlink_container").hide();
        });

      $('.hide-filter-effect label input[value="2"]').parent().click('on',
        function () {
          //$("#settings_triggerlink_container").show();
        });

      $('.drill_accordion > ul  > li.drillaccordionback > .backlink').on('click', function () {
        $('.start_end_view').show();
        $('.start_end_view').removeClass('display-none');
        if (!drill_id) {
          return false;
        }
        $('#' + drill_id + ' > ul li.drillaccordionlink').show();
        $('#' + drill_id + ' > ul li.drillaccordionback').hide();
        $('#' + drill_id + ' > .drillaccordioncontent').hide();
        $('#' + drill_id + ' > .drillaccordionshowactive').hide();
      });
    }
  </script>
