  <script type="text/javascript">
    // Load the initial settings into the settings variable
    var settings = {{{settingsJSON}}};

    // Load the component ID into the compId variable
		var compId = getParameterByName('compId');

		/* Listen for changes to the settings */
		Wix.addEventListener(Wix.Events.SETTINGS_UPDATED, onSettingsUpdate, compId);

    /* When settings are updated */
		function onSettingsUpdate(update) {
      // Update the settings variable
			settings = update;

      // Remove the html body class
      $('body').attr('class','');

      // Inject styles into the stylesheet
			$('#forcestyle').html('');
			$('#forcestyle').append('.unblur .face0 img, .blur .face1 img { -webkit-filter: blur(' + (update.blurLevel > 1 ? update.blurLevel/6 : update.blurLevel) + 'px);	-moz-filter:blur(' +  (update.blurLevel > 1 ? update.blurLevel/6 : update.blurLevel) + 'px);filter:blur(' +  (update.blurLevel > 1 ? update.blurLevel/6 : update.blurLevel) + 'px);}');
			$('#forcestyle').append('.unblur .face0 .tv , .blur .face1 .tv2  { -webkit-filter: blur(' + (update.blurLevel > 1 ? update.blurLevel/6 : update.blurLevel) + 'px);	-moz-filter:blur(' +  (update.blurLevel > 1 ? update.blurLevel/6 : update.blurLevel) + 'px);filter:blur(' +  (update.blurLevel > 1 ? update.blurLevel/6 : update.blurLevel) + 'px);}');
			$('#forcestyle').append('.unopaque .face0 {	opacity:' +  (update.opacityLevel > 1 ? update.opacityLevel/100 : update.opacityLevel) + ';}');
			$('#forcestyle').append('.hover.opaque .face1 {opacity:' +  (update.opacityLevel > 1 ? update.opacityLevel/100 : update.opacityLevel) + ';}  .preview.opaque .face1 {opacity:' +  (update.opacityLevel > 1 ? update.opacityLevel/100 : update.opacityLevel) + ' !important;}');
			$('#forcestyle').append('.zoom .card {-webkit-transition: all .2s ease-in-out;	transition: all .2s ease-in-out;	-webkit-transform: scale(' + (update.zoomLevel > 2 ? 1+(update.zoomLevel/50) : update.zoomLevel) + ');transform: scale(' +  (update.zoomLevel > 2 ? 1+(update.zoomLevel/50) : update.zoomLevel) + ');} ');
			$('#forcestyle').append('.hover.unzoom .card, .preview.unzoom .card {-webkit-transform: scale(' +  (update.zoomLevel > 1 ? 1-(update.zoomLevel/100) : update.zoomLevel) + ');transform: scale(' + (update.zoomLevel > 1 ? 1-(update.zoomLevel/100) : update.zoomLevel) + ');}');
			$('#forcestyle').append('.hover.twistl .card, .preview.twistl .card {-webkit-transform: scale(' +  (update.zoomLevel > 2 ? 1+(update.zoomLevel/50) : update.zoomLevel) + ')  rotate(-' +  update.twistLevel + 'deg);transform: scale(' +  (update.zoomLevel > 2 ? 1+(update.zoomLevel/50) : update.zoomLevel) + ')  rotate(-' +  update.twistLevel + 'deg);}');
			$('#forcestyle').append( '.hover.twistr .card , .preview.twistr .card{-webkit-transform: scale(' +  (update.zoomLevel > 2 ? 1+(update.zoomLevel/50) : update.zoomLevel) + ')  rotate(' +  update.twistLevel + 'deg);transform: scale(' +  (update.zoomLevel > 2 ? 1+(update.zoomLevel/50) : update.zoomLevel) + ') rotate(' +  update.twistLevel + 'deg);} ' );

      if(settings.imageZoomOff) {
        $('#codyhouse-image0').addClass('imagezoom');
      } else {
        $('#codyhouse-image0').removeClass('imagezoom');
      }

			/****Check codrops Zoom effect *****/
			if(settings.transitiontype=="codrops"){
				if(settings.imageZoomOn) {
					if(settings.textbtna==true || settings.textbtna=='true'){
						$(".codrops-modal-trigger").click('on',function(){
								$('#codrops-image1').addClass('imagezoom');
						});

						$(".codrops-close").click('on',function(){
								$('#codrops-image1').removeClass('imagezoom');
								$("#codrops .content--intro").css('-webkit-transform','translateY(0vh)');
						});
					}else{
							$(".codrops-close").hide();
							$('body').mouseleave(function(){
										$('#codrops-image1').removeClass('imagezoom');
							});
							$('body').mouseenter(function(){
									$('#codrops-image1').addClass('imagezoom');
							});
					}
				} else {
				 	$('#codrops-image1').removeClass('imagezoom');
				}
				if (settings.image1) {
						if(settings.image1[0]==undefined){
								$('#codrops-image1').hide();
						}else{
								$('#codrops-image1').show();
						}
				}else{
					console.log('dsds');
					$('#codrops-image1').hide();
				}

			}
			/****Check codrops Zoom effect *****/

      // Load or reload the widget
			if (widgetload) {
					widgetload(update);
			}
		}

	</script>

