      <script>
        $(document).ready(function () {
          // Image controls
          var imageOffControl = $('#image0').getCtrl();
          var imageOnControl = $('#image1').getCtrl();

          var buttons = [
            {
              label: 'Change',
              icon: 'image-change',
              onClick: function () {
                Wix.Settings.openMediaDialog(Wix.Settings.MediaType.IMAGE, false, function (data) {
                  imageOffControl.setValue([data]);
                  image_trigger('image0', [data]);
                   var img0Ext = data.relativeUri.split('.').pop();
                   if(img0Ext!='png'){
                      $("#background-colour-off").hide();
                   }
                   if(img0Ext=='png'){
                      console.log('show');
                      $("#background-colour-off").show();
                   }
                });
              }
            },
            {
              label: 'Remove',
              icon: 'delete',
              onClick: function () {
                imageOffControl.setValue([]);
                $("#background-colour-off").show();
                setTimeout(saveAndRefresh, 100);
              }
            }
          ];

          imageOffControl.setButtons(buttons);

          // Image controls
          var buttons = [
            {
              label: 'Change',
              icon: 'image-change',
              onClick: function () {
                Wix.Settings.openMediaDialog(Wix.Settings.MediaType.IMAGE, false, function (data) {
                  imageOnControl.setValue([data]);
                  var img1Ext = data.relativeUri.split('.').pop();
                   if(img1Ext!='png'){
                       $("#background-colour-on").hide();
                   }
                  image_trigger('image1', [data]);
                });
              }
            },
            {
              label: 'Remove',
              icon: 'delete',
              onClick: function () {
                imageOnControl.setValue([]);
                $("#background-colour-on").show();
                setTimeout(saveAndRefresh, 100);
              },
            }
          ];

          imageOnControl.setButtons(buttons);
        });
      </script>

